<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.BeerPhotos">
    <select id="selectOne" resultType="BeerPhoto">
        select 
            beer_photo_id as id,
            url,
            width,
            height 
        from 
            beer_photos 
        limit 1;
    </select>
</mapper>