<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.repository.BeerPhotoRepository">
    <select id="selectAll" resultType="com.example.demo.model.BeerPhoto">
        select 
            beer_photo_id as id,
            url,
            width,
            height 
        from 
            beer_photos 
        order by
            beer_photo_id;
    </select>
</mapper>
